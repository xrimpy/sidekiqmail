<h1>Listing emailers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @emailers.each do |emailer| %>
      <tr>
        <td><%= link_to 'Show', emailer %></td>
        <td><%= link_to 'Edit', edit_emailer_path(emailer) %></td>
        <td><%= link_to 'Destroy', emailer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Emailer', new_emailer_path %>
